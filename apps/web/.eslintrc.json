{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "project": ["./tsconfig.json"],
    "tsconfigRootDir": "./"
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": ["warn", {"allowExpressions": true}],
    "@typescript-eslint/no-explicit-any": "off"
  }
}
